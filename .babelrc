{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-3",
    "stage-2",
    "react"
  ],
  "retainLines": true,
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "transform-decorators-legacy",
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}